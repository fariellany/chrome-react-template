{
  "manifest_version": 2,
  "name": "Chrome Cookie Proxy",
  "background": {
    "persistent": true,
    "scripts": [
      "background.bundle.js"
    ]
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "icon-34.png"
  },
  "icons": {
    "128": "icon-128.png"
  },
  "permissions": [
    "tabs",
    "proxy",
    "cookies",
    "storage",
    "bookmarks",
    "webRequest",
    "webRequestBlocking",
    "*://*/*"
  ],
  "web_accessible_resources": [
    "content.styles.css",
    "icon-128.png",
    "icon-34.png"
  ]
}